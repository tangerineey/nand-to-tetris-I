/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=sel, out=NOTsel);
    And(a=NOTsel, b=a, out=aANDNOTsel);
    And(a=sel, b=b, out=selANDb);
    Xor(a=aANDNOTsel, b=selANDb, out=out);
}